    "{.,{src,stories,test}/**}/**/*.{s,}css": "stylelint"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "last 2 versions",
    "IE >= 11"
