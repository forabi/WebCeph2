    "dev": "HOT=1 NODE_ENV=development webpack-dev-server",
    "clean": "rm -rf ./build/",
    "build": "DEBUG=build NODE_ENV=production webpack",
    "stats": "NODE_ENV=production webpack --json > stats.json"
  },
  "lint-staged": {
    "{.,{src,stories,test}/**}/*.{j,t}s{x,}": "prettier --write --single-quote --trailing-comma all",
